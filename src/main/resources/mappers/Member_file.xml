<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Member_fileMapper">
     <!-- Beans의 맴버변수(property)이름과 대상 테이블의 컬럼(column)을 연결한다. -->
     <resultMap type="kr.co.semo.model.Maemul" id="maemulMap">
        <result property="id" column="id"/>
        <result property="origin_name" column="item_addrst"/> 
        <result property="file_name" column="item_addrnd"/>
        <result property="content_type" column="select_type"/>
        <result property="file_size" column="select_type"/>
        <result property="reg_date" column="select_type"/>
        <result property="edit_date" column="select_type"/>
        <result property="file_type" column="select_type"/>
        <result property="co_member_id" column="co_member_id"/>
     </resultMap>
     
     <!-- 데이터 저장을 위한 기능 정의 -->
     <insert id="insertItem"
     parameterType="kr.co.semo.model.Member_file"
     keyProperty="id"
     useGeneratedKeys="true">
		INSERT INTO member_file ( oring_name, file_name, content_type, file_size, reg_date,
			edit_date, file_type, co_member_id
		) VALUES (
	        #{oring_name}, #{file_name}, #{content_type}, #{file_size}, #{reg_date},
			#{edit_date}, #{file_type}, #{co_member_id}
        )
     </insert>
           
  </mapper>